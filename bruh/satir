local OurComputer
for i,v in pairs(game.Workspace.Computers:GetChildren()) do
    for i2,v2 in pairs(game.Workspace.Studios[game.Players.LocalPlayer.Name .. "'s" .. ' Studio'].Items:GetChildren()) do
        if string.match(v.Name, v2.Name) then
            OurComputer = v2
        end
    end
end

local RunService = game:GetService('RunService')
_G.Enabled = true
RunService.RenderStepped:Connect(function()
    if _G.Enabled then
        game.ReplicatedStorage.fileMade:FireServer()
        wait()
        game.ReplicatedStorage.singleVideo:FireServer(OurComputer)
    end
end)
